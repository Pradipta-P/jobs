listView('Devops') {
    description('All Devops jobs for project A')
    filterBuildQueue()
    filterExecutors()
    jobs {
        name('DSL_Pipeline')
        name('Test_DSL_Pipeline')
    }
    columns {
        status()
        weather()
        name()
        lastSuccess()
        lastFailure()
        lastDuration()
        buildButton()
    }

}
